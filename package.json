{
  "name": "cypress-test-tiny",
  "version": "1.0.0",
  "description": "Small project for quickly testing Cypress.io test runner",
  "scripts": {
    "cypress:run": "cypress run",
    "cypress:run:windows": "SET \"CYPRESS_CONFIG_FILE=qa\" && SET \"CYPRESS_SOME_VAR=someValue\" && cypress run",
    "cypress:run:nix": "CYPRESS_CONFIG_FILE=qa CYPRESS_SOME_VAR=someValue cypress run",
    "cypress:run:cli-env": "cypress run --env CONFIG_FILE=qa,SOME_VAR=someValue",
    "cypress:open": "cypress open",
    "cypress:verify": "cypress verify",
    "cypress:version": "cypress version",
    "list-videos": "ls -l cypress/videos",
    "check-videos": "ok-file 'cypress/videos/*.mp4'"
  },
  "keywords": [],
  "author": "Gleb Bahmutov <gleb@cypress.io>",
  "license": "ISC",
  "devDependencies": {
    "commit-message-install": "1.10.1",
    "cypress": "^3.6.1",
    "ok-file": "1.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cypress-io/cypress-test-tiny.git"
  }
}
